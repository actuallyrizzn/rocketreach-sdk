{
    "name": "rocketreach/sdk",
    "description": "PHP SDK for RocketReach People Search API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "RocketReach SDK Team",
            "email": "sdk@rocketreach.co"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "RocketReach\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RocketReach\\SDK\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
